{
  "name": "rabbit-hole-3-installer",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "docker build . -t rabbit-hole-3-api:1.0",
    "start": "docker compose up -d",
    "beautify": "cd app && npx prettier . --write && cd ..\\api && venv\\Scripts\\python.exe -m black .",
    "check": "git diff --exit-code --quiet"
  },
  "pre-commit": {
    "run": ["beautify", "check"]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "pre-commit": "^1.2.2"
  }
}
